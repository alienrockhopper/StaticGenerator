  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File with Arguments",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "args": [
        "./config/Earth2Test.json"
      ]
    },
    {
      "name": "Python Debugger: RunMain",
      "type": "debugpy",
      "request": "launch",
      "program": "main.py",
      "console": "integratedTerminal",
      "args": [
        "./config/SnowBoarder.json"
      ]
    }
  ]
}